<keymap version="1" name="GNOME copy" parent="Default for GNOME">
  <action id="Back">
    <keyboard-shortcut first-keystroke="shift alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt comma" />
  </action>
  <action id="CopyElement" />
  <action id="EditorCreateRectangularSelectionOnMouseDrag">
    <mouse-shortcut keystroke="shift alt button1" />
  </action>
  <action id="EditorPasteFromX11" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="shift alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt period" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <mouse-shortcut keystroke="control alt button1" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="Resume">
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="ShowBookmarks" />
  <action id="StepInto">
    <keyboard-shortcut first-keystroke="f11" />
  </action>
  <action id="StepOut">
    <keyboard-shortcut first-keystroke="shift f11" />
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="ToggleBookmark" />
</keymap>